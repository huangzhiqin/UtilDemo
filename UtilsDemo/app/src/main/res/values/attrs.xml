<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="recycler_swipe_SwipeMenuLayout">
        <attr name="leftViewId" format="reference"/>
        <attr name="rightViewId" format="reference"/>
        <attr name="contentViewId" format="reference"/>
    </declare-styleable>

    <declare-styleable name="NoPaddingTextView">
        <attr name="textSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="RatingBarStyle" >
        <attr name="ratingImageSize" format="dimension"/>
        <attr name="ratingWidth" format="dimension"/>
        <attr name="ratingHeight" format="dimension"/>
        <attr name="ratingCount" format="integer"/>
        <attr name="ratingImagePadding" format="dimension"/>
        <attr name="ratingImageEmpty" format="reference"/>
        <attr name="ratingStartHalf" format="reference"/>
        <attr name="ratingImageFull" format="reference"/>
        <attr name="half" format="boolean"/>
        <attr name="dividerDrawable" format="reference"/>
    </declare-styleable>

    <declare-styleable name="RatingBarView">
        <attr name="starImageSize" format="dimension" />
        <attr name="starCount" format="integer" />
        <attr name="starEmpty" format="reference" />
        <attr name="starFill" format="reference" />

    </declare-styleable>
</resources>